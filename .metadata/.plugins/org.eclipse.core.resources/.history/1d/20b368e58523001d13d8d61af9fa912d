CREATE TABLE map(
mid				BIGINT			PRIMARY KEY AUTO_INCREMENT,
businessNumber	VARCHAR(13)		NOT NULL,
longitude		DOUBLE			NOT NULL,
latitude		DOUBLE			NOT NULL,
CONSTRAINT map_FK
	FOREIGN KEY(businessNumber) REFERENCES shop(businessNumber)
)

SELECT * FROM map;

INSERT INTO map VALUES(mid, "123-456-789", 35.865886, 128.595598); 시류
INSERT INTO map VALUES(mid, "111-11-1111", 35.8713156, 128.5988512); 크로바
INSERT INTO map VALUES(mid, "000-000-000",35.8719847, 128.5973511); 헤기
DROP TABLE map
SELECT * FROM map WHERE name like '%영희%'